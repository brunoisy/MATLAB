filename = 'data/MAT_clean/data_4/curve_1.mat';
load(filename)
xlimits = [-10, 150];
ylimits = [-200, 50];


[Lc, Xsel, Fsel, Xfirst, Xunfold,delta] = LSQ_fit_fd(dist, force, 4, true, 10, 10, 5);

delta


figure
hold on
xlim(xlimits);
ylim(ylimits);
plot(dist+delta, force,'.');
for i = 1:length(Lc)
    Xfit = linspace(0,Lc(i)+delta,1000);
    Ffit = fd(Lc(i)delta, Xfit);
    plot(Xfit,Ffit,'Color',colors(mod(i,7)+1,:));
end

